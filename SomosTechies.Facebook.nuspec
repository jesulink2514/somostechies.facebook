<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/01/nuspec.xsd">
  <metadata>
    <id>somostechies.facebook</id>
    <version>1.1.0</version>
    <description>
      Native Facebook Login for Xamarin.Forms
    </description>
    <iconUrl>https://github.com/jesulink2514/somostechies.facebook/raw/master/logo.png</iconUrl>
    <authors>Jesus Angulo</authors>
    <projectUrl>https://github.com/jesulink2514/somostechies.facebook</projectUrl>

    <dependencies>

      <group targetFramework="MonoAndroid7.0">
        <dependency id="Xamarin.Facebook.Android" version="4.26.0"/>
      </group>

      <group targetFramework="Xamarin.iOS1.0">
        <dependency id="Xamarin.Facebook.iOS" version="4.27.1" />
      </group>
    </dependencies>

    <references>
      <group targetFramework=".NETStandard2.0">
        <reference file="SomosTechies.Facebook.dll" />
      </group>
      <group targetFramework="Xamarin.iOS1.0">
        <reference file="SomosTechies.Facebook.dll" />
        <reference file="SomosTechies.Facebook.iOS.dll" />
      </group>
      <group targetFramework="MonoAndroid7.0">
        <reference file="SomosTechies.Facebook.dll" />
        <reference file="SomosTechies.Facebook.Android.dll" />
      </group>
    </references>
  </metadata>
  <files>
    <file src="bin/netstandard2.0/SomosTechies.Facebook.dll" target="lib\netstandard2.0" />
    <file src="bin/Release/SomosTechies.Facebook.Android.dll" target="lib\MonoAndroid70" />
    <file src="bin/Release/SomosTechies.Facebook.iOS.dll" target="lib\Xamarin.iOS10" />
  </files>
</package>